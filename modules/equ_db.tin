#action {Created: Tue Apr  2 16:00:26 2024} {}
#action {Type: %1} {show_equ_match;#var equ[type] %1}
#action {Worn on: %1} {show_equ_match;#var equ[worn] %1}
#action {Features: %1} {show_equ_match;#var equ[features] %1}
#action {Composition: %1} {show_equ_match;#var equ[composition] %1}
#action {Restricted: !Evil} {show_equ_match;#var equ[restricted] %1}
#action {Weight: %1 stones, Value: %2 coins, Size: %3} {show_equ_match;#var equ[weight] %1;#var equ[value] %2;#var equ[size] %3}

#action {Max AC-apply: %1, Current AC-apply: %2} {show_equ_match;#var equ[ac][max] %1;#var equ[ac][current] %2}

