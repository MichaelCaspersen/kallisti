#action {  +%1 Luck} {show_equ_match;#var equ[affects][luck] %1}


#action {Minimum level:} {}
#action { Fighter  : %1   Rogue   : %2   Priest : %3   Monastic: %4   Mage       : %5 } {show_equ_match;#var equ[minlevel][fighter] %1;#var equ[minlevel][rogue] %2;#var equ[minlevel][priest] %3;#var equ[minlevel][monastic] %4;#var equ[minlevel][mage] %5}
#action { Barbarian: %1   Assassin: %2   Templar: %3   Monk    : %4   Wizard     : %5 } {show_equ_match;#var equ[minlevel][barbarian] %1;#var equ[minlevel][assassin] %2;#var equ[minlevel][templar] %3;#var equ[minlevel][monk] %4;#var equ[minlevel][wizard] %5}
#action { Paladin  : %1   Demoniac: %2   Druid  : %3   Samurai : %4   Necromancer: %5 } {show_equ_match;#var equ[minlevel][paladin] %1;#var equ[minlevel][demoniac] %2;#var equ[minlevel][druid] %3;#var equ[minlevel][samurai] %4;#var equ[minlevel][necromancer] %5}
#action { Dreadlord: %1   Bard    : %2   Prophet: %3 } {show_equ_match;#var equ[minlevel][dreadlord] %1;#var equ[minlevel][bard] %2;#var equ[minlevel][prophet] %3}
#action { Ranger   : %1 } {show_equ_match;#var equ[minlevel][ranger] %1}
#action { Valkyrie : %1 } {show_equ_match;#var equ[minlevel][valkyrie] %1}

#action {Requirements:} {}
#action {            Strength  Dexterity  Rank (%1)} {show_equ_match;#var equ[weapon][skill] %1}

#action {Wield :      %1         %2       %3} {show_equ_match;#var equ[weapon][wield][str] %1;#var equ[weapon][wield][dex] %2;#var equ[weapon][wield][rank] %3}
#action {Hold  :      %1       %2       %3} {show_equ_match;#var equ[weapon][hold][str] %1;#var equ[weapon][hold][dex] %2;#var equ[weapon][hold][rank] %3}



#alias show_equ_match {#show <131>Triggered:}
